<resources xmlns:tools="http://schemas.android.com/tools">


    <style name="Eyepetizer" parent="Theme.AppCompat.Light.NoActionBar" tools:ignore="ResourceName">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/base_colorWhite</item>
        <item name="colorPrimaryDark">@color/base_colorWhite</item>
        <item name="colorAccent">@color/base_colorAccent</item>
    </style>

    <style name="Eyepetizer.NoActionBar" tools:ignore="ResourceName">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Eyepetizer.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" tools:ignore="ResourceName" />

    <style name="Eyepetizer.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" tools:ignore="ResourceName" />

    <style name="Eyepetizer.SettingTextView" tools:ignore="ResourceName">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">28dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginTop">45dp</item>
        <item name="android:paddingStart">5dp</item>
        <item name="android:paddingEnd">5dp</item>
        <item name="android:gravity">center</item>
        <item name="typeface">fzlLTypeface</item>
    </style>

    <style name="Eyepetizer.MineTextView" parent="Eyepetizer.SettingTextView" tools:ignore="ResourceName">
        <item name="android:layout_height">25.5dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginTop">45dp</item>
    </style>
</resources>